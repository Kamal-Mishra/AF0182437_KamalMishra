
public class multip_Num {

	public static void main(String[] args) {
		int table1=24;
		int table2=50;
		int table3=29;
		for (int i=1; i<=10; i++)
		{
			System.out.println(table1+" * "+i+" = " +table1*i);
			System.out.println(table2+" * "+i+" = " +table2*i);
			System.out.println(table3+" * "+i+" = " +table3*i);
		}
	}

}
